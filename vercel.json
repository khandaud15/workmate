{
  "functions": {
    "app/api/jobs/search/route.ts": {
      "maxDuration": 300
    },
    "app/api/jobs/status/route.ts": {
      "maxDuration": 30
    },
    "app/api/jobs/results/route.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/jobs/search",
      "dest": "/api/jobs/search"
    },
    {
      "src": "/api/jobs/status",
      "dest": "/api/jobs/status"
    },
    {
      "src": "/api/jobs/results",
      "dest": "/api/jobs/results"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
